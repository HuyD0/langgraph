# Example configuration file for LangGraph MCP Agent
# Copy this to .env and customize for your environment

# Databricks Configuration
DATABRICKS_PROFILE=development
# DATABRICKS_HOST=https://your-workspace.cloud.databricks.com

# Model Configuration
MODEL_ENDPOINT_NAME=databricks-claude-3-7-sonnet
MODEL_SYSTEM_PROMPT=You are a helpful assistant that can run Python code.

# MCP Server URLs (comma-separated)
# MCP_MANAGED_URLS=https://workspace.com/api/2.0/mcp/functions/system/ai
# MCP_CUSTOM_URLS=https://custom-mcp-server.com/mcp

# MLflow Configuration
MLFLOW_EXPERIMENT_NAME=/Users/your.email@example.com/langgraph-mcp-agent
MLFLOW_ENABLE_AUTOLOG=true

# Unity Catalog Configuration
UC_CATALOG=rag
UC_SCHEMA=development
UC_MODEL_NAME=langgraph_mcp_agent

# Deployment Configuration
DEPLOYMENT_SCALE_TO_ZERO_ENABLED=true
